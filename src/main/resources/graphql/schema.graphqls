type Query {
    getArticles(categoryId: ID): [Article]
    getArticle(id: ID): Article
    category: Category
}

type Article {
    id: ID
    title: String
    content: String
    category: Category
    beforeArticles: [Article]
    afterArticle: [Article]
}

type Category {
    id: ID
    name: String
    description: String
}